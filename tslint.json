{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rules": {
    "interface-name": [true, "never-prefix"],
    "curly": [true, "as-needed"],
    "no-shadowed-variable": false,
    "ban-types": [false],
    "object-literal-sort-keys": false,
    "no-console": false
  }
}
