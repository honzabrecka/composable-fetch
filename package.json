{
  "name": "composable-fetch",
  "version": "5.0.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:honzabrecka/composable-fetch.git"
  },
  "author": "jan.brecka@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.18",
    "@types/node-fetch": "^2.1.4",
    "flow-bin": "^0.79.1",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^23.6.0",
    "rapid-check": "^0.2.0",
    "transit-js": "^0.8.861",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  },
  "scripts": {
    "tsc": "tsc",
    "jest": "jest",
    "tslint": "tslint -c tslint.json '{index.ts,__tests__/**/*.ts}' -t verbose",
    "flow:copy": "cp index.js.flow dist",
    "build": "yarn tsc && yarn flow:copy"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
