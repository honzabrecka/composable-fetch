{
  "name": "composable-fetch",
  "version": "4.0.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:honzabrecka/composable-fetch.git"
  },
  "author": "jan.brecka@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.0.0",
    "@types/node": "^10.3.2",
    "flow-bin": "^0.74.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^23.1.0",
    "rapid-check": "^0.2.0",
    "transit-js": "^0.8.861",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "typescript": "^2.9.1"
  },
  "scripts": {
    "tsc": "tsc",
    "jest": "jest",
    "tslint": "tslint -c tslint.json '{index.ts,__tests__/**/*.ts}' -t verbose",
    "flow:copy": "cp index.js.flow dist",
    "build": "yarn tsc && yarn flow:copy"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
